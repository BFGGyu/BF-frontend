{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts", ".tsx", ".native.js"]
      }
    }
  },
  // 검사무시 추가
  "ignorePatterns": ["node_modules/*"]
}
